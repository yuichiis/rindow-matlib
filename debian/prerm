#!/bin/sh
set -e

if [ "$1" != "upgrade" ]
then
    update-alternatives --remove librindowmatrib.so \
                        /usr/lib/rindowmatrib-serial/librindowmatrib.so

    update-alternatives --remove librindowmatrib.so \
                        /usr/lib/rindowmatrib-openmp/librindowmatrib.so
fi



exit 0
